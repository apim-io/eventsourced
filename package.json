{
  "name": "eventsourced",
  "version": "1.0.8",
  "description": "An Event Sourcing library for Node using ES6, Immutable, NLP and some CQRS.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lgomez/eventsourced.git"
  },
  "scripts": {
    "test": "tap -R spec lib/**/*.spec.js",
    "cov": "npm t -- -R classic --cov",
    "start": "nodemon -q --exec 'npm t'",
    "docs": "documentation -f md -o ./docs/entity.md build ./lib/entity/entity.js",
    "preversion": "eslint . && npm t",
    "postversion": "git push && git push --tags && npm publish"
  },
  "keywords": [
    "event sourcing",
    "event",
    "source",
    "sourcing",
    "immutable",
    "cqrs"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "Luis Gomez <lgomez@gmail.com> (http://luis.io)",
  "license": "MIT",
  "dependencies": {
    "immutable": "^3.8.1",
    "immutablediff": "^0.4.2",
    "immutablepatch": "^0.2.2",
    "nlp_compromise": "^4.10.5"
  },
  "devDependencies": {
    "documentation": "^4.0.0-beta2",
    "eslint": "^2.10.1",
    "eslint-config-airbnb-base": "^3.0.1",
    "eslint-plugin-import": "^1.8.0",
    "nodemon": "^1.9.2",
    "tap": "^5.7.1"
  }
}
