{
  "name": "eventsourced",
  "version": "1.0.12",
  "description": "An Event Sourcing library for Node using ES6, Immutable, NLP and some CQRS.",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:lgomez/eventsourced.git"
  },
  "scripts": {
    "start": "nodemon -q --exec 'npm t'",
    "test": "tap -R spec lib/**/*.spec.js",
    "cov": "npm t -- -R classic --cov",
    "docs": "documentation -g -f md -o ./docs/entity.md build ./lib/entity/entity.js && git add docs/* && git ci -m 'Updated documentation' docs/*",
    "postversion": "git push && git push --tags"
    "preversion": "standard && npm t && npm run docs",
  },
  "keywords": [
    "event sourcing",
    "event",
    "source",
    "sourcing",
    "immutable",
    "cqrs"
  ],
  "engines": {
    "node": ">=6.0.0"
  },
  "author": "Luis Gomez <lgomez@gmail.com> (http://luis.io)",
  "license": "MIT",
  "dependencies": {
    "documentation": "^4.0.0-beta9",
    "immutable": "^3.8.1",
    "immutablediff": "^0.4.3",
    "immutablepatch": "^0.2.2",
    "nlp_compromise": "^4.12.1",
    "nodemon": "^1.10.2",
    "tap": "^5.8.0"
  },
  "devDependencies": {
    "documentation": "^4.0.0-beta2",
    "nodemon": "^1.9.2",
    "standard": "^8.0.0",
    "tap": "^5.7.1"
  }
}
